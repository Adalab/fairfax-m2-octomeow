{
  "icons": {
    "src": "_src/assets/icons/*.*",
    "dest": "public/",
    "dist": "docs/"
  },
  "html": {
    "src": "_src/templates/*.html",
    "partials": "_src/templates/partials/",
    "dest": "public/",
    "dist": "docs/"
  },
	"scss": {
		"src": "_src/assets/scss/**/*.scss",
		"dest": "public/assets/css/",
    "dist": "docs/assets/css/"
	},
	"js": {
		"src": [
      "_src/assets/js/main.js",
      "_src/assets/js/collapsibles.js",
      "_src/assets/js/preview-colors.js",
      "_src/assets/js/preview-github.js",
      "_src/assets/js/preview-job.js",
      "_src/assets/js/preview-linkedin.js",
      "_src/assets/js/preview-name.js",
      "_src/assets/js/preview-tel.js",
      "_src/assets/js/share.js",
      "_src/assets/js/img-load.js",
      "_src/assets/js/object.js"   
		],
		"dest": "public/assets/js/",
    "dist": "docs/assets/js/"
	},
	"images": {
    "src": "_src/assets/images/**/*.{gif,jpg,jpeg,JPG,JPEG,png,svg}",
    "dest": "public/assets/images/",
    "dist": "docs/assets/images/"
  },
  "watch": {
   "html": "_src/**/*.html"
  }
}
